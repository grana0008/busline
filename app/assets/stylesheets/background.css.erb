#bg-image {
height: 41px;
background: url("<%= asset_path 'Soft White.jpg' %>");
background-repeat: no-repeat;
background-position: left top, right bottom, left top;

-webkit-border-radius: 3px;
-moz-border-radius: 3px;
border-radius: 3px;

-webkit-box-shadow: .5px 15px 10px -10px rgba(0, 0, 0, 1), 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.2) inset;
-moz-box-shadow: .5px 15px 10px -10px rgba(0, 0, 0, 1), 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.2) inset;
box-shadow: .5px 15px 10px -10px rgba(0, 0, 0, 1), 0 1px 4px rgba(0, 0, 0, 0.3), 0 0 40px rgba(0, 0, 0, 0.2) inset;
}

div.navbar { 
 margin-bottom: 20px;
}

#img-size {
	width: 140px;
	height: 140px;
}